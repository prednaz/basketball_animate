{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "after-used"
      }
    ]
  },
  "globals": {
    "svg_main": "readonly",
    "initialize": "readonly",
    "player_to_move_start": "readonly",
    "player_to_move_destination": "readonly",
    "player_move": "readonly",
    "pass": "readonly",
    "shoot": "readonly",
    "timeline": "readonly",
    "animation_supplementary": "readonly",
    "clock": "readonly",
    "time": "readonly",
    "player_possession_set": "readonly",
    "svg": "readonly",
    "music_set": "readonly",
    "svg_set": "readonly",
    "bpm_set": "readonly",
    "path_shorten": "readonly",
    "play_button": "readonly",
    "play_button_label": "readonly",
    "position_slider": "readonly",
    "position_display_slider": "readonly",
    "library": "readonly",
    "Power0": "readonly",
    "Power1": "readonly",
    "TimelineMax": "readonly",
    "TweenMax": "readonly",
    "$": "readonly",
    "Snap": "readonly"
  }
}
